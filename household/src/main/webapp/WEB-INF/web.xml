<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SellerRegister</servlet-name>
  	<display-name>SellerRegister</display-name>
  	<description></description>
  	<servlet-class>com.seller.servlet.SellerRegister</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Seller</servlet-name>
  	<display-name>Seller</display-name>
  	<description></description>
  	<servlet-class>com.entity.Seller</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DBConnect</servlet-name>
  	<display-name>DBConnect</display-name>
  	<description></description>
  	<servlet-class>com.db.DBConnect</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SellerDao</servlet-name>
  	<display-name>SellerDao</display-name>
  	<description></description>
  	<servlet-class>com.dao.SellerDao</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SellerRegister</servlet-name>
  	<url-pattern>/SellerRegister</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Seller</servlet-name>
  	<url-pattern>/Seller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DBConnect</servlet-name>
  	<url-pattern>/DBConnect</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SellerDao</servlet-name>
  	<url-pattern>/SellerDao</url-pattern>
  </servlet-mapping>
</web-app>
